<!DOCTYPE html>
<html lang="en">

<%- include('partials/header.ejs') %>


<body>

    <%- include('partials/navbar.ejs') %>

    <div class="section no-pad-bot" id="index-banner">

        <br><br>
        <h1 class="header center title">
            ʻ<%= title %>ʼ
        </h1>
        <div class="row center">
            <h5 class="header col s12 light">
                <%= description %>
        </div>
        <br>
        <hr style="width: 50%;">

    </div>

    <div class="section no-pad-bot" id="index-banner">


        <div class="row center">
            <% if (price === "Free") { %>
            <h4 class="header col s12 light">
                <%= price %>
            </h4>
            <% } else if (price === "Basic") { %>
            <div class="row center">
                <div class="container">
                    <div class="card light-blue lighten-1 center">
                        <div class="card-content white-text">
                            <h2 class="title-fontfamily"><%= price %></h2>
                            <ul>
                                <li>
                                    <h5>Get 360 request per minute for animals EndPoints</h5>
                                </li>
                                <li>
                                    <h5>Get 360 request per minute for Anime EndPoints</h5>
                                </li>
                                <li>
                                    <h5>Get 20000 requests per minute for utils EndPoints</h5>
                                </li>
                                <li>
                                    <h5>Get 120 requests per minute for /Upload EndPoint</h5>
                                </li>
                                <li>
                                    <h5>Support via email or Discord</h5>
                                </li>
                            </ul>

                            <br>

                            <h3 class="title-fontfamily">Pay via:</h3>


                            <!-- Make a button for buy subscription -->

                            <a class="btn-large waves-effect waves-light blue darken-1" href="<%= protocol %>://<%= host %>/buy?price=Basic&email=<%= email %>&name=<%= name %>&project=<%= typeOfProject %>">
                                <i class="material-icons left">payment</i> 
                                <span>PayPal</span>
                            </a>

                        </div>
                    </div>
                </div>
            </div>
            <% } else if (price === "Pro") { %>

            <div class="row center">
                <div class="container">
                    <div class="card light-blue lighten-1 center">
                        <div class="card-content white-text">
                            <h2 class="title-fontfamily"><%= price %></h2>
                            <ul>
                                <li>
                                    <h5>Get 360 request per minute for animals EndPoints</h5>
                                </li>
                                <li>
                                    <h5>Get 360 request per minute for Anime EndPoints</h5>
                                </li>
                                <li>
                                    <h5>Get 20000 requests per minute for utils EndPoints</h5>
                                </li>
                                <li>
                                    <h5>Get 120 requests per minute for /Upload EndPoint</h5>
                                </li>
                                <li>
                                    <h5>Support via email or Discord</h5>
                                </li>
                            </ul>

                            <br>

                            <h3 class="title-fontfamily">Pay via:</h3>




                        </div>
                    </div>
                </div>
            </div>
            <% } else if (price === "Enterprise") { %>
            <h4 class="header col s12 light">
                <%= price %>
            </h4>
            <% } else { %>
            <h4 class="header col s12 light">
                Well well well, you are not supposed to be here.
            </h4>
            <% } %>
        </div>

    </div>




    <%- include('partials/footer.ejs') %>

    <script src="js/apikey.js"></script>

</body>




</html>